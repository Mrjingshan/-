.container{
  background: #FFFFFF;
}
.messageContent{
  width: 100%;
  background: #FFFFFF;
}
.messageLi{
  width: 710rpx;
  margin: 0 auto;
  padding: 14rpx 0;
  border-bottom: 1rpx solid #D8D8D8;
}
.messageLi:last-child{
  border-bottom: none;
}
.messageTitle{
  height: 80rpx;
  font-size: 28rpx;
  color: #666666;
  line-height: 40rpx;
  vertical-align: middle;
}
.messageTitle>text{
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background: #E51A24;
  vertical-align: middle;
  margin: -5rpx 18rpx 0 0;
}
.messageDate{
  width: 100%;
  font-size: 26rpx;
  color: #999999;
  line-height: 36rpx;
  text-align: right;
}
.cleanBtn{
  width: 158rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  /* border: 1rpx solid #535353; */
  border-radius: 16rpx;
  position: fixed;
  bottom: 38rpx;
  right: 20rpx;
  font-size: 28rpx;
  color: #fff;
  background: #E51A24;
}
.messageNull>image{
  width: 360rpx;
  height: 196rpx;
  display: block;
  margin: 178rpx auto 0;
}
.messageNull>view{
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666666;
  margin: 28rpx auto 0;
}