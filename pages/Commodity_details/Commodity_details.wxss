page{
  width: 100%;
  background: #fff;
}
.viewContent{
  width: 100%;
  height: 92%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}
button {
  background-color: #fff;
  border: none;
}
button[disabled] {
  color: #fff !important;
  background: #ccc !important;
  border-color: #ccc !important;
}
button[disabled].bg1{
  border-right: solid 1px #fff !important;
}
.indexFixed{
  position: fixed;
  top:0;
  left:0;
  bottom:0;
  right:0;
}
input {
  outline: none;
  border: none;
  list-style: none;
}

button::after {
  border: none;
}

