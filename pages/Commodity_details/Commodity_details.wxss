.stepper {
	width: 158rpx;
	height: 46rpx;
  display: flex;
	/*给主容器设一个边框*/
	border: 1px solid #ccc;
	border-radius: 3px;
  overflow: hidden;
}
 
/*加号和减号*/
.stepper button {
	width: 54rpx;
	display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  border: none;
  resize: none;
  color: black;
}
 
/*数值*/
button::after {
  border: none;
}
.stepper .nums {
	width: 50rpx;
	height: 46rpx;
  display: flex;
  justify-content: center;
  align-items: center;
	font-size: 12px;
	/*给中间的input设置左右边框即可*/
	border-left: 1rpx solid #ccc;
	border-right: 1rpx solid #ccc;
}
 
/*普通样式*/
.stepper .normal{
	color: black;
}
 
/*禁用样式*/
.stepper .disabled{
	color: #ccc;
}
