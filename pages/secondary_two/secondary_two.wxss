/* pages/index/secondaryType/secondary_two/secondary_two.wxss */
page{
  background: #f5f5f5;
}
.person_box{
  width: 100%;
}
.person_box_list{
  width: 100%;
  box-sizing: border-box;
  padding: 37rpx 20rpx;
  border-bottom: solid 1px #ebebeb;
  background: #fff;
}
.person_box_list_top{
  width: 100%;
  display: flex;
}
.person_box_list_top_img{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.person_box_list_top_img image{
  width: 100%;
  height: 100%;
}
.person_box_list_top_msg{
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  align-items: space-between;
  margin-left: 20rpx;
}
.person_box_list_top_msg_name{
  font-size: 16px;
  color: #333333;
  margin-top: 10rpx;
  font-weight: bold;
}
.person_box_list_top_msg_text{
  width: 594rpx;
  height: 40rpx;
  font-size: 14px;
  color: #666666;
  white-space: nowrap;  
  text-overflow:ellipsis; 
  overflow:hidden;
  margin-top: 10rpx;
}
.person_box_list_bottom{
  width: 100%;
  display: flex;
  overflow-x: auto;
  margin-top: 20rpx;
}
.person_box_list_bottom_li{
  width: 210rpx;
  margin-left: 20rpx;
}
.person_box_list_bottom_li:nth-child(1){
  margin-left: 0rpx;
}
.person_box_list_bottom_li image{
  width: 210rpx;
  height: 190rpx;
}
.person_box_list_bottom_name{
  width: 100%;
  font-size: 15px;
  color: #666666;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.person_box_list_bottom_price{
  font-size: 17px;
  color: #E51A24;
}
.backHomeImg{
  width: 116rpx;
  height: 116rpx;
  position: fixed;
  bottom: 28rpx;
  left: 50%;
  margin-left: -58rpx;
}