.bgBox {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.bgBox>image {
  width: 100%;
  height: 100%;
}

.contentBox {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.headBox {
  width: 432rpx;
  margin: 56rpx auto 0;
  vertical-align: middle;
  overflow: hidden;
  padding-bottom: 32rpx;
  border-bottom: 6rpx solid #FFFFFF;
}

.headImg {
  width: 100rpx;
  height: 100rpx;
  border: 2px solid #fff;
  border-radius: 50%;
  float: left;
  box-shadow: 0 2px 2px 0 rgba(129, 129, 129, 0.53);
}
.headImg>image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.headText{
  width: 310rpx;
  height: 100%;
  float: right;
}
.headText>view{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #666666;
}
.headText>view.headTextSub{
  font-size: 34rpx;
  color: #000000;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;

}
.headTitle{
  width: 100%;
  font-size: 30rpx;
  color: #000000;
  font-weight: bold;
  text-align: center;
  margin: 54rpx auto;
}
.headAds{
  width: 594rpx;
  margin: 0 auto;
}
.headAds1{
  position: relative;
  font-size: 32rpx;
  color: #333333;
  line-height: 60rpx;
}
.headAds1>image{
  width: 16rpx;
  height: 20rpx;
  position: absolute;
  top: 20rpx;
  left: -25rpx;
}
.headAds2{
  margin-top: 40rpx;
  font-size: 16px;
  color: #666666;
  line-height: 50rpx;
}
.keyBtn{
  width: 690rpx;
  height: 94rpx;
  background: #E51A24;
  line-height: 94rpx;
  border: 1px solid rgba(5,5,5,0.08);
  border-radius: 5px;
  font-size: 18px;
  color: #FFFFFF;
  text-align: center;
  margin: 80rpx auto 0;
}
.bottomContent{
  width: 100%;
  line-height: 40rpx;
  font-size: 14px;
  color: #333333;
  margin-top: 129rpx;
  text-align: center;
}
.bottomContent>image{
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 22rpx;
}
.keyText{
  width: 594rpx;
  font-size: 16px;
  height: 50rpx;
  line-height: 50rpx;
  margin: 12rpx auto 0;
  color: #E51A24;
  font-weight: bold;
}
.adLogo{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  bottom: 160rpx;
  left: -100rpx;
}