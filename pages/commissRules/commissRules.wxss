.container {
  background: #fff;
}

.content {
  width: 690rpx;
  margin: 34rpx auto 0;
}

.head {
  width: 100%;
  height: 77rpx;
  overflow: hidden;
  display: flex;
  border: 1rpx solid #999;
}
.head:first-child{
  border-bottom: none;
}
.head .item {
  flex: 1;
  text-align: center;
  line-height: 77rpx;
  font-size: 26rpx;
  color: #666;
  border-right: 1rpx solid #999;
}

.head .item:last-child {
  border-right: none;
}

.rulesTip {
  width: 630rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #999;
  position: fixed;
  top: 518rpx;
  left: 68rpx;
}
