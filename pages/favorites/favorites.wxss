.favHead{
  width: 100%;
  height: 72rpx;
  overflow: hidden;
  font-size: 30rpx;
  color: #333333;
  line-height: 72rpx;
}
.favCount{
  display: inline-block;
  margin-left: 30rpx;
  float: left;
}
.favOption{
  display: inline-block;
  margin-right: 30rpx;
  float: right;
}
scroll-view{
  width: 750rpx;
  position: relative;
  overflow: hidden;
  background: #FFFFFF;
}
.favLi:first-child{
  margin-top: 6rpx;
}
.favLi{
  width: 690rpx;
  height: 200rpx;
  margin: 0 auto;
  padding: 30rpx 0;
  overflow: hidden;
  border-bottom: 1rpx solid #DFDFDF;
}
.favLiLeft image{
  width: 290rpx;
  height: 200rpx;
  border-radius: 5px;
  display: inline-block;
  float: left;
}
.favLiRight{
  display: inline-block;
  height: 100%;
  float: right;
}
.favName{
  height: 50rpx;
  font-size: 32rpx;
  color: #333333;
  line-height: 48rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.favPrice{
  font-size: 28rpx;
  color: #e51a24;
  margin-top: 0rpx;
}
.favCheck{
  width: 66rpx;
  height: 100%;
  float: left;
  display: inline-block;
}
checkbox .wx-checkbox-input{
    border-radius: 50%;
    height: 30rpx;
    width: 30rpx;
    margin: 82rpx 36rpx 0 0;
    background: #F5F5F5;
    border: 1px solid #C7C7C7;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 22rpx;
  color: #FFFFFF;
  border-radius: 50%;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
  background-color: #E51A24;
  border: 1rpx solid #E51A24;
}
.bottomOption{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  vertical-align: middle;
  overflow: hidden;
  font-size: 30rpx;
  color: #333333;
  background: #FFFFFF;
}
checkbox.allCheck .wx-checkbox-input{
  vertical-align: middle;
  margin: -5rpx 36rpx 0 30rpx;
}
.bottomOption text{
  width: 188rpx;
  height: 60rpx;
  font-size: 30rpx;
  color: #666666;
  display: inline-block;
  border-radius: 30rpx;
  margin: 20rpx 20rpx 0 0;
  line-height: 60rpx;
  text-align: center;
  border: 1rpx solid #666666;
  float: right;
}
.nullContant{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #F5F5F5;
}
.nullData{
  width: 100%;
  height: 360rpx;
  background: #FFFFFF;
  border: 1rpx solid transparent;
}
.nullData image{
  width: 360rpx;
  height: 196rpx;
  display: block;
  margin: 40rpx auto;
}
.nullData>view{
  width: 100%;
  height: 30rpx;
  font-size: 30rpx;
  color: #666666;
  line-height: 30rpx;
  text-align: center;
}
.recommend{
  width: 100%;
  margin-top: 20rpx;
}
.recommendHead{
  width: 100%;
  height: 80rpx;
  background: #FFFFFF;
  position: relative;
}
.lineLeft{
  width: 242rpx;
  height: 6rpx;
  background: #E51A24;
  position: absolute;
  top: 38rpx;
  left: 30rpx;
}
.lineRight{
  width: 242rpx;
  height: 6rpx;
  background: #E51A24;
  position: absolute;
  top: 38rpx;
  right: 30rpx;
}
.lineCenter{
  width: 180rpx;
  height: 40rpx;
  position: absolute;
  top: 20rpx;
  right: 284rpx;
  font-size: 28rpx;
  color: #E51A24;
  vertical-align: middle;
}
.lineCenter image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin: -5rpx 8rpx 0 0;
}
.showFav{
  overflow: hidden;
}
.showFavLi{
  width: 369rpx;
  height: 406rpx;
  display: inline-block;
  background: #FFFFFF;
  float: left;
  margin-right: 8rpx;
}
.showFavLi:nth-of-type(even){
  margin-right: 0;
}
.showFavLi image{
  width: 100%;
  height: 278rpx;
  display: block;
}
.showFavLiName{
  width: 359rpx;
  padding-left: 10rpx;
  font-size: 30rpx;
  line-height: 42rpx;
  color: #333333;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.showFavLiStyle{
  width: 359rpx;
  padding-left: 10rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #999999;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.showFavLiPrice{
  width: 359rpx;
  padding-left: 10rpx;
  font-size: 28rpx;
  color: #E51A24;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.showFavLiPrice>text{
  width: 100%;
  font-size: 40rpx;
}
.Popular_recommendation{
  width: 100%;
  margin-top: 40rpx;
}
.Popular_recommendation_title{
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
  margin-top: -12rpx;
}
.xiantiao{
  width: 242rpx;
  height: 3rpx;
  background: #E51A24;
}
.Popular_recommendation_title_center{
  display: flex;
  align-items: center;
  font-size: 14px;
  color: #E51A24;
}
.Popular_recommendation_title_center image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.onbox_tabshow{
  /* width: 100%; */
  column-count: 2;
  column-gap: 20rpx;
  margin-top: 20rpx;
  box-sizing: border-box;
  padding: 0rpx 20rpx;
}
.tabbox{
  background-color: #fff;
  break-inside: avoid;
  margin-top: 10rpx;
  /* padding-bottom: 20rpx; */
}
.tabbox:nth-child(1){
  margin-top: 0px;
}
.tabbox:nth-child(2){
  margin-top: 0px;
}
.tabbox image{
  width: 100%;
  height: 264rpx;
}
.tabbox_title{
  font-size: 15px;
  color: #333333;
  margin-left: 10rpx;
}
.tabbox_titletwo{
  font-size: 12px;
  color: #999999;
  margin-left: 10rpx;
}
.tabbox_price{
  font-size: 17px;
  color: #E51A24;
  margin-left: 10rpx;
}
.car_NoData_top{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  box-sizing: border-box;
  padding: 100rpx 0rpx;
}
.car_NoData_top image{
  width: 360rpx;
  height: 202rpx;
}
.text{
  font-size: 15px;
  color: #666666;
}