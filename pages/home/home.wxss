.container {
  background: #fff;
  z-index: 999;
}

.homeBox {
  width: 600rpx;
  height: 392rpx;
  margin: 116rpx auto 0;
}

.homeBox>image {
  width: 100%;
  height: 100%;
}

.homeDesc {
  width: 100%;
  height: 40rpx;
  margin-top: 84rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #999;
}

.getUserBtn {
  width: 690rpx;
  height: 92rpx;
  text-align: center;
  line-height: 92rpx;
  background: #66c410;
  border-radius: 8rpx;
  margin: 30rpx auto 0;
}

.getUserBtn>image {
  width: 100%;
  height: 100%;
}

.getPhoneBtn {
  width: 690rpx;
  height: 92rpx;
  text-align: center;
  line-height: 92rpx;
  background: #fff;
  border-radius: 8rpx;
  margin: 30rpx auto 0;
  font-size: 32rpx;
  color: #333;
  border: 1rpx solid #979797;
}
/* 招商页面 */
.homePageAdContent{
  border: 1rpx solid red;
}
.bgBox{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.bgBox>image {
  width: 100%;
  height: 100%;
}
.contentBox {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border: 1rpx solid red;
}

.headBox {
  width: 432rpx;
  margin: 56rpx auto 0;
  vertical-align: middle;
  overflow: hidden;
  padding-bottom: 32rpx;
  border-bottom: 6rpx solid #FFFFFF;
}

.headImg {
  width: 100rpx;
  height: 100rpx;
  border: 2px solid #fff;
  border-radius: 50%;
  float: left;
  box-shadow: 0 2px 2px 0 rgba(129, 129, 129, 0.53);
}
.headImg>image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.headText{
  width: 310rpx;
  height: 100%;
  float: right;
}
.headText>view{
  line-height: 50rpx;
  font-size: 28rpx;
  color: #666666;
}
.headText>view.headTextSub{
  font-size: 34rpx;
  color: #000000;
}
.headTitle{
  width: 100%;
  font-size: 30rpx;
  color: #000000;
  font-weight: bold;
  text-align: center;
  margin: 54rpx auto;
}
.headAds{
  width: 594rpx;
  margin: 0 auto;
}
.headAds1{
  position: relative;
  font-size: 32rpx;
  color: #333333;
  line-height: 60rpx;
}
.headAds1>image{
  width: 16rpx;
  height: 20rpx;
  position: absolute;
  top: 20rpx;
  left: -25rpx;
}
.headAds2{
  margin-top: 40rpx;
  font-size: 16px;
  color: #666666;
  line-height: 50rpx;
}
.keyBtn{
  width: 690rpx;
  height: 94rpx;
  background: #E51A24;
  line-height: 94rpx;
  border: 1px solid rgba(5,5,5,0.08);
  border-radius: 5px;
  font-size: 18px;
  color: #FFFFFF;
  text-align: center;
  margin: 80rpx auto 0;
}
.bottomContent{
  width: 100%;
  line-height: 40rpx;
  font-size: 14px;
  color: #333333;
  margin-top: 198rpx;
  text-align: center;
}
.bottomContent>image{
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 22rpx;
}